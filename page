import { Request } from "./Request";
import { Result } from "./Result";
import { void_t } from "./void";
import { objectId } from "./objectId";
import { Value } from "./value";

export type createPageResult : Result {
  objectId id;
}

export call CreatePage : Request => createPageResult {
  string title;
}

trait InputPageItem {}

type inputPageItemLog : InputPageItem {
    vector<Value> args;
}

export call AddPageItem : Request => void_t {
    objectId pageId;
    InputPageItem item;
}
